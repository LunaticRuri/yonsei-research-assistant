# Environment Variables Template
# Copy this file to .env and fill in your actual values

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Google API Configuration
GOOGLE_API_KEY=your_google_api_key_here

# Database Configuration
CHROMA_DB_PATH=./data/chroma_db

# Service URLs (for local development)
DIALOGUE_SERVICE_URL=http://localhost:8001
STRATEGY_SERVICE_URL=http://localhost:8002
RAG_SERVICE_URL=http://localhost:8003
SEARCH_AGENT_SERVICE_URL=http://localhost:8004

# Yonsei Library Configuration
YONSEI_LIBRARY_BASE_URL=https://library.yonsei.ac.kr
SCRAPING_DELAY=1.0

# Logging Configuration
LOG_LEVEL=INFO

# Session Configuration
SESSION_TIMEOUT=3600

# CORS Configuration
CORS_ORIGINS=["http://localhost:3000"]

# Caching Configuration
ENABLE_CACHING=true
CACHE_TTL=300